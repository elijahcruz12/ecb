{
    "name": "elijahcruz/ecb",
    "description": "Deploy your project on your server with a single command",
    "keywords":  ["deploy", "cli"],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Elijah Cruz",
            "email": "elijah@ec-ws.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel-zero/framework": "^9.0",
        "nunomaduro/termwind": "^1.3",
        "ext-yaml": "*"
    },
    "require-dev": {
        "laravel/pint": "^0.2.0",
        "mockery/mockery": "^1.4.4",
        "pestphp/pest": "^1.21.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["ecb"]
}
